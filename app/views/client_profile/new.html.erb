<%= form_for :client_profile, url: {action: 'create'}  do |f|%>
  <%= f.label :first_name, "first name" %>
  <%= f.text_field :first_name  %>
  <%= f.label :last_name, "last name" %>
  <%= f.text_field :last_name  %>

  <br>

  <%= f.fields_for :user_attributes do |ff| %>
    <div>
      <%= ff.label :email, 'email' %>
      <%= ff.text_field :email  %>
      <br>
      <%= ff.label :address, 'address' %>
      <%= ff.text_field :address %>
      <br>
      <%= ff.label :password, 'password' %>
      <%= ff.password_field :password %>
      <br>
      <%= ff.label :password_confirmation, 'password confirmation' %>
      <%= ff.password_field :password_confirmation %>
      <br>
    </div>
  <% end %>
  <%= f.submit 'Submit' %>
<% end %>